      "parameters": {
        "jsCode": "return items.map(item => {\n  const url = $('When Executed by Another Workflow').first().json.url;\n  const cleanUrl = url.replace(/^https?:\\/\\//, '');\n  \n  const results = item.json.organic_results || [];\n  const found = results.find(r => r.link.includes(cleanUrl));\n  const current_rank = found\n    ? found.position\n    : 'not found in top 10';\n  return { json: { current_rank }};\n});"
